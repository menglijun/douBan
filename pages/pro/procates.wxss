/* pages/pro/procates.wxss */
.container{
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.login-container{
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 400rpx;
}
.login-container .login{
    width: 300rpx;
    height: 80rpx;
    line-height: 80rpx;
    background-color: white;
    text-align: center;
    border-radius: 40rpx;
}
.login-container .login-image{
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: -1;
}
.version{
    width: 100%;
    color: grey;
    font-size: 30rpx;
    position: absolute;
    bottom: 50rpx;
    justify-content: center;
    text-align: center;
}